{
  "name": "poel-frontend",
  "description": "vpro poel front end scripts",
  "scripts": {
    "vendor:handlebars": "cp node_modules/handlebars/dist/handlebars.min.js src/main/resources/static/vendor",
    "postvendor:handlebars": "cp node_modules/handlebars/dist/handlebars.runtime.min.js src/main/resources/static/vendor",
    "vendor:jquery": "cp node_modules/jquery/dist/jquery.min.js src/main/resources/static/vendor",
    "vendor:plugin-hbs": "cp node_modules/plugin-hbs/hbs.js src/main/resources/static/vendor",
    "vendor:stapes": "cp node_modules/stapes/stapes.min.js src/main/resources/static/vendor",
    "vendor:systemjs": "cp node_modules/systemjs/dist/system.js src/main/resources/static/vendor",
    "vendor:traceur": "cp node_modules/traceur/bin/traceur.js src/main/resources/static/vendor",
    "vendor": "npm run vendor:handlebars && npm run vendor:jquery && npm run vendor:stapes && npm run vendor:systemjs && npm run vendor:traceur && npm run vendor:plugin-hbs"
  },
  "dependencies": {
    "handlebars": "^4.0.5",
    "jquery": "^2.2.3",
    "plugin-hbs": "github:vpro/plugin-hbs",
    "stapes": "^0.8.1",
    "systemjs": "^0.19.27",
    "systemjs-builder": "^0.15.16",
    "traceur": "0.0.108"
  }
}
